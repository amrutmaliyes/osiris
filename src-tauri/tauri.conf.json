{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "osiris",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
			"window": {
				"all": true
			},
			"path": {
				"all": true
			},
			"shell": {
				"all": true
			},
			"protocol": {
				"all": true,
				"assetScope": ["*"]
			},
			"os": {
				"all": true
			},
			"dialog": {
				"all": true,
				"open": true,
				"save": true
			}
    },
    "bundle": {
      "active": true,
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.osiris.dev",
      "targets": "all"
    },
    "security": {
      "csp": "default-src 'self'; img-src 'self' asset: https://asset.localhost"
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "maximized": true,
        "title": "osiris"
      }
    ]
  }
}
